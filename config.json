{
    "title": "MNIST_Sample",
    "track_experiment": false,

    "wandb": {
        "group": "",
        "project": "mnist-jax-sample",
        "id": "test-run-2",
        "entity": "jeekangaroo"
    },


    "model_struct": {
        "model": "mnist_model",
        "args": {
            "feat_dims": [8, 16, 32]
        }
    },


    "data_loader": {
        "loader": "DataLoader",
        "args": {
            "dataset_args": {
                "root": "./data"
            },
            "batch_size": 512,
            "shuffle": true
        },

        "test_loader": "DataLoader",
        "test_args": {
            "dataset_args": {
                "root": "./data",
                "test": true
            },
            "batch_size": 512,
            "shuffle": true
        }
    },


    "training": {
        "optimizer": "adamw",
        "optimizer_args": {
            "lr": 0.003
        },

        "loss_fn": "batch_cross_entropy",

        "metrics": ["accuracy"],

        "epochs": 20,

        "checkpoint": "",

        "save_dir": "saved/",
        "save_period": 2
    }
}